FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RASModel        kEpsilon;

turbulence      on;

printCoeffs     on;

kEpsilonCoeffs
{
    Sc              1;
    Sct             0.7;
    Cchi            2;
    Cmu             0.09;
    C1              1.44;
    C2              1.84;
    C3              -0.33;
    sigmak          1;
    sigmaEps        1.3;
    Prt             1;
}


